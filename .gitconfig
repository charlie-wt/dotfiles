[alias]
    a  = add
    br = branch
    c  = commit -am
    ci = commit
    co = checkout
    d  = diff
    l  = log --graph --oneline --abbrev-commit --decorate --all
    last = log -1 HEAD
    ll = log --graph --decorate --all
    mg = merge
    s  = status
    trunk = log --first-parent master
    unstage = reset HEAD --
[merge]
    tool = vimdiff
[mergetool "vimdiff"]
    cmd = "vimdiff -O $LOCAL $MERGED $REMOTE"
